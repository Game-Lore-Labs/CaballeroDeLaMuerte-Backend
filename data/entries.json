    [191, {"entryDataId": 191, "entryDataNarrative": "Te lanzas al ataque.", "entryDataOptions": [{"id": 1, "description": "¡Combatir!", "outcome": {"type": "StartCombat", "enemies": ["CaballeroDeLaMuerte"], "victory": 200, "defeat": 210}}], "entryDataRules": []}],
    [192, {"entryDataId": 192, "entryDataNarrative": "\"¿Quién eres tú para darme órdenes? ¡Prepárate para morir!\"", "entryDataOptions": [{"id": 1, "description": "¡Combatir!", "outcome": {"type": "StartCombat", "enemies": ["CaballeroDeLaMuerte"], "victory": 200, "defeat": 210}}], "entryDataRules": []}],
    [193, {"entryDataId": 193, "entryDataNarrative": "Preparas la Estaca del Árbol Rojo. Si vences, podrás destruirlo permanentemente.", "entryDataOptions": [{"id": 1, "description": "¡Combatir!", "outcome": {"type": "StartCombat", "enemies": ["CaballeroDeLaMuerte"], "victory": 201, "defeat": 210}}], "entryDataRules": []}],
    [200, {
        "entryDataId": 200,
        "entryDataNarrative": "¡El Caballero de la Muerte cae! Añade 200 XP.\n\nPero su mano se contrae... ¡no está destruido! Sin la estaca, debes huir con Darek.",
        "entryDataOptions": [
            {"id": 1, "description": "Huir con Darek", "outcome": {"type": "GoToEntry", "entry": 220}}
        ],
        "entryDataRules": []
    }],
    [201, {
        "entryDataId": 201,
        "entryDataNarrative": "¡El Caballero de la Muerte cae! Añade 200 XP.\n\nClavas la Estaca del Árbol Rojo en su pecho. ¡Se desmorona en polvo! Un grito sobrenatural llena la cámara mientras su espíritu es liberado.\n\n\"Gracias... gracias por salvarme\", dice Darek.",
        "entryDataOptions": [
            {"id": 1, "description": "Liberar a Darek y salir", "outcome": {"type": "GoToEntry", "entry": 230}}
        ],
        "entryDataRules": []
    }],
    [210, {
        "entryDataId": 210,
        "entryDataNarrative": "El Caballero de la Muerte te derrota. Libera a Darek para que huya, luego te toca la frente.\n\n\"Has demostrado ser digno. Tu destino se ha aclarado.\"\n\nSientes la vida abandonándote... y una nueva energía entrando.\n\n\"¡Ahora levántate como mi ESCUDERO!\"\n\n¡Te has convertido en el Escudero del Caballero de la Muerte!\n\n[FIN - DESTINO OSCURO]",
        "entryDataOptions": [],
        "entryDataRules": []
    }],
    [220, {
        "entryDataId": 220,
        "entryDataNarrative": "Huyes con Darek. Llegas a Orlbar y lo entregas a sus abuelos.\n\nLady Brewmont te da un collar de 700 po.\n\nPero el Caballero de la Muerte no está destruido. Volverá...\n\n[VICTORIA PARCIAL]",
        "entryDataOptions": [
            {"id": 1, "description": "Ver recompensas", "outcome": {"type": "GoToEntry", "entry": 240}}
        ],
        "entryDataRules": []
    }],
    [230, {
        "entryDataId": 230,
        "entryDataNarrative": "Entregas a Darek a sus abuelos. Lord Brewmont te paga 2000 po.\n\nHas completado tu misión. El Caballero de la Muerte ha sido destruido para siempre.\n\n¡VICTORIA TOTAL!",
        "entryDataOptions": [
            {"id": 1, "description": "Ver recompensas", "outcome": {"type": "GoToEntry", "entry": 240}}
        ],
        "entryDataRules": []
    }],
    [240, {
        "entryDataId": 240,
        "entryDataNarrative": "BOTÍN DE AVENTURA\n\nHas completado \"El Escudero del Caballero de la Muerte\".\n\nTu personaje avanza al NIVEL 3.\n\n¡Gracias por jugar!",
        "entryDataOptions": [],
        "entryDataRules": []
    }],
    [300, {
        "entryDataId": 300,
        "entryDataNarrative": "¡Ves movimiento adelante! Tres figuras pequeñas emergen de la maleza: ¡KOBOLDS! Te ven y gritan, avanzando rápidamente hacia ti con dagas y bolas de fuego.",
        "entryDataOptions": [
            {"id": 1, "description": "¡Combatir!", "outcome": {"type": "StartCombat", "enemies": ["Kobold", "Kobold", "KoboldMago"], "victory": 301, "defeat": 999}}
