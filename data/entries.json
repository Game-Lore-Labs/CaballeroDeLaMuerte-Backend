    [162, {"entryDataId": 162, "entryDataNarrative": "Algo se mueve bajo la tierra... ¡te han detectado! El suelo de una tumba se agrieta.", "entryDataOptions": [{"id": 1, "description": "Ir a la cripta rápidamente", "outcome": {"type": "GoToEntry", "entry": 320}}, {"id": 2, "description": "Huir", "outcome": {"type": "GoToEntry", "entry": 125}}], "entryDataRules": []}],
    [163, {"entryDataId": 163, "entryDataNarrative": "Notas una tumba con tierra fresca. La evitas.", "entryDataOptions": [{"id": 1, "description": "Ir a la cripta", "outcome": {"type": "GoToEntry", "entry": 165}}], "entryDataRules": []}],
    [164, {"entryDataId": 164, "entryDataNarrative": "No detectas nada. Avanzas confiado hacia la cripta cuando... ¡el suelo cede!", "entryDataOptions": [{"id": 1, "description": "¡Saltar! (Salvación DES CD 16)", "outcome": {"type": "SaveCheck", "attribute": "Dexterity", "dc": 16, "success": 165, "failure": 325}}], "entryDataRules": []}],
