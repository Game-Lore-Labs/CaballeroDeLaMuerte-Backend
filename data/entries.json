    [186, {
        "entryDataId": 186,
        "entryDataNarrative": "Ves una cámara con un trono. En él, un guerrero espantoso de siete pies: ¡EL CABALLERO DE LA MUERTE! Ante él, atado, está Darek Brewmont.\n\n¡Has llegado sin ser detectado!",
        "entryDataOptions": [
            {"id": 1, "description": "Atacar por sorpresa a distancia", "outcome": {"type": "GoToEntry", "entry": 190}},
            {"id": 2, "description": "Atacar cuerpo a cuerpo", "outcome": {"type": "GoToEntry", "entry": 191}},
            {"id": 3, "description": "Intentar hablar", "outcome": {"type": "GoToEntry", "entry": 192}}
        ],
        "entryDataRules": [
            {"ruleDataCondition": {"tag": "HasItem", "contents": "estaca_arbol_rojo"}, "ruleDataEffect": {"type": "AddOption", "option": {"id": 99, "description": "Preparar la Estaca del Árbol Rojo", "outcome": {"type": "GoToEntry", "entry": 193}}}}
        ]
    }],
    [187, {
        "entryDataId": 187,
        "entryDataNarrative": "El Caballero de la Muerte te ve. \"¡ASÍ QUE! ¡El gran héroe! ¡Prepárate para morir, necio!\"",
        "entryDataOptions": [
            {"id": 1, "description": "¡Combatir!", "outcome": {"type": "StartCombat", "victory": 200, "defeat": 210}}
        ],
        "entryDataRules": []
    }],
    [190, {"entryDataId": 190, "entryDataNarrative": "Tu ataque golpea al caballero. \"¡Aaargh! ¿Quién se atreve?\" Se levanta furioso.", "entryDataOptions": [{"id": 1, "description": "¡Combatir!", "outcome": {"type": "StartCombat", "victory": 200, "defeat": 210}}], "entryDataRules": []}],
    [191, {"entryDataId": 191, "entryDataNarrative": "Te lanzas al ataque.", "entryDataOptions": [{"id": 1, "description": "¡Combatir!", "outcome": {"type": "StartCombat", "victory": 200, "defeat": 210}}], "entryDataRules": []}],
    [192, {"entryDataId": 192, "entryDataNarrative": "\"¿Quién eres tú para darme órdenes? ¡Prepárate para morir!\"", "entryDataOptions": [{"id": 1, "description": "¡Combatir!", "outcome": {"type": "StartCombat", "victory": 200, "defeat": 210}}], "entryDataRules": []}],
    [193, {"entryDataId": 193, "entryDataNarrative": "Preparas la Estaca del Árbol Rojo. Si vences, podrás destruirlo permanentemente.", "entryDataOptions": [{"id": 1, "description": "¡Combatir!", "outcome": {"type": "StartCombat", "victory": 201, "defeat": 210}}], "entryDataRules": []}],
    [200, {
        "entryDataId": 200,
        "entryDataNarrative": "¡El Caballero de la Muerte cae! Añade 200 XP.\n\nPero su mano se contrae... ¡no está destruido! Sin la estaca, debes huir con Darek.",
        "entryDataOptions": [
            {"id": 1, "description": "Huir con Darek", "outcome": {"type": "GoToEntry", "entry": 220}}
        ],
        "entryDataRules": []
    }],
    [201, {
        "entryDataId": 201,
        "entryDataNarrative": "¡El Caballero de la Muerte cae! Añade 200 XP.\n\nClavas la Estaca del Árbol Rojo en su pecho. ¡Se desmorona en polvo! Un grito sobrenatural llena la cámara mientras su espíritu es liberado.\n\n\"Gracias... gracias por salvarme\", dice Darek.",
        "entryDataOptions": [
            {"id": 1, "description": "Liberar a Darek y salir", "outcome": {"type": "GoToEntry", "entry": 230}}
        ],
        "entryDataRules": []
    }],
    [210, {
