    [141, {"entryDataId": 141, "entryDataNarrative": "Detrás del altar encuentras un Símbolo Sagrado de Amaunator.", "entryDataOptions": [{"id": 1, "description": "Buscar más", "outcome": {"type": "GoToEntry", "entry": 142}}, {"id": 2, "description": "Salir", "outcome": {"type": "GoToEntry", "entry": 125}}], "entryDataRules": []}],
    [142, {"entryDataId": 142, "entryDataNarrative": "Encuentras un libro con información sobre el Caballero de la Muerte y un Pergamino de Curar Heridas.", "entryDataOptions": [{"id": 1, "description": "Salir", "outcome": {"type": "GoToEntry", "entry": 125}}], "entryDataRules": []}],
    [143, {"entryDataId": 143, "entryDataNarrative": "No encuentras nada de valor.", "entryDataOptions": [{"id": 1, "description": "Inspeccionar altar", "outcome": {"type": "GoToEntry", "entry": 141}}, {"id": 2, "description": "Salir", "outcome": {"type": "GoToEntry", "entry": 125}}], "entryDataRules": []}],
    [150, {"entryDataId": 150, "entryDataNarrative": "El bosque termina. ¡Pero no has encontrado a Darek! Debe estar en otra parte.", "entryDataOptions": [{"id": 1, "description": "Volver", "outcome": {"type": "GoToEntry", "entry": 125}}, {"id": 2, "description": "Abandonar la misión", "outcome": {"type": "GoToEntry", "entry": 999}}], "entryDataRules": []}],
