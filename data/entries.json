    [131, {"entryDataId": 131, "entryDataNarrative": "Tallas una rama hasta hacer una estaca afilada.\n\n¡Has obtenido una Estaca del Árbol Rojo!", "entryDataOptions": [{"id": 1, "description": "Volver al sendero", "outcome": {"type": "GoToEntry", "entry": 125}}], "entryDataRules": []}],
    [132, {"entryDataId": 132, "entryDataNarrative": "Desde arriba ves afloramientos rocosos al norte.", "entryDataOptions": [{"id": 1, "description": "Tomar una rama", "outcome": {"type": "GoToEntry", "entry": 131}}, {"id": 2, "description": "Volver al sendero", "outcome": {"type": "GoToEntry", "entry": 125}}], "entryDataRules": []}],
