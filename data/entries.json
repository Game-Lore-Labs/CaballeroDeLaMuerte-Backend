    [161, {"entryDataId": 161, "entryDataNarrative": "Te mueves silenciosamente.", "entryDataOptions": [{"id": 1, "description": "Ir a la cripta", "outcome": {"type": "GoToEntry", "entry": 165}}], "entryDataRules": []}],
    [162, {"entryDataId": 162, "entryDataNarrative": "Algo se mueve bajo la tierra... ¡te han detectado!", "entryDataOptions": [{"id": 1, "description": "Ir a la cripta", "outcome": {"type": "GoToEntry", "entry": 165}}, {"id": 2, "description": "Huir", "outcome": {"type": "GoToEntry", "entry": 125}}], "entryDataRules": []}],
    [163, {"entryDataId": 163, "entryDataNarrative": "Notas una tumba con tierra fresca. La evitas.", "entryDataOptions": [{"id": 1, "description": "Ir a la cripta", "outcome": {"type": "GoToEntry", "entry": 165}}], "entryDataRules": []}],
    [164, {"entryDataId": 164, "entryDataNarrative": "No detectas nada.", "entryDataOptions": [{"id": 1, "description": "Ir a la cripta", "outcome": {"type": "GoToEntry", "entry": 165}}], "entryDataRules": []}],
    [165, {
        "entryDataId": 165,
