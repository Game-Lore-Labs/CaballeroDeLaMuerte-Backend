            {"id": 1, "description": "Tomar una rama (crear estaca)", "outcome": {"type": "GoToEntry", "entry": 131}},
            {"id": 2, "description": "Trepar (Atletismo CD 9)", "outcome": {"type": "SkillCheck", "skill": "Athletics", "dc": 9, "success": 132, "failure": 133}},
