    [175, {"entryDataId": 175, "entryDataNarrative": "Dentro de la cueva hay huesos y cadáveres. En la pared dice: \"NORTE\". Encuentras un medallón: \"A Fastus Brant, Capitán de la Guardia de Orlbar\".", "entryDataOptions": [{"id": 1, "description": "Buscar tesoros (Investigación CD 12)", "outcome": {"type": "SkillCheck", "skill": "Investigation", "dc": 12, "success": 176, "failure": 310}}, {"id": 2, "description": "Salir", "outcome": {"type": "GoToEntry", "entry": 125}}], "entryDataRules": []}],
