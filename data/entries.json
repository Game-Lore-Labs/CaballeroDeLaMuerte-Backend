    [170, {"entryDataId": 170, "entryDataNarrative": "Una cueva al otro lado de un río.", "entryDataOptions": [{"id": 1, "description": "Vadear (Atletismo CD 12)", "outcome": {"type": "SkillCheck", "skill": "Athletics", "dc": 12, "success": 171, "failure": 172}}, {"id": 2, "description": "Volver", "outcome": {"type": "GoToEntry", "entry": 125}}], "entryDataRules": []}],
    [171, {"entryDataId": 171, "entryDataNarrative": "Cruzas el río.", "entryDataOptions": [{"id": 1, "description": "Entrar a la cueva", "outcome": {"type": "GoToEntry", "entry": 175}}], "entryDataRules": []}],
    [172, {"entryDataId": 172, "entryDataNarrative": "La corriente te arrastra. Pierdes 1 PV y tus provisiones.", "entryDataOptions": [{"id": 1, "description": "Entrar a la cueva", "outcome": {"type": "GoToEntry", "entry": 175}}], "entryDataRules": []}],
    [175, {"entryDataId": 175, "entryDataNarrative": "Dentro de la cueva hay huesos y cadáveres. En la pared dice: \"NORTE\". Encuentras un medallón: \"A Fastus Brant, Capitán de la Guardia de Orlbar\".", "entryDataOptions": [{"id": 1, "description": "Salir", "outcome": {"type": "GoToEntry", "entry": 125}}], "entryDataRules": []}],
    [180, {
        "entryDataId": 180,
        "entryDataNarrative": "Dentro de la cripta hay cuatro ataúdes. Al fondo, un pasillo continúa.",
