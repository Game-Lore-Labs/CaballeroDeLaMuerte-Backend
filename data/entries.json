    [122, {"entryDataId": 122, "entryDataNarrative": "¡Activas la trampa! Dardos vuelan hacia ti.", "entryDataOptions": [{"id": 1, "description": "Esquivar (Salvación DES CD 14 con ventaja)", "outcome": {"type": "SaveCheck", "attribute": "Dexterity", "dc": 14, "success": 121, "failure": 124}}], "entryDataRules": []}],
    [124, {"entryDataId": 124, "entryDataNarrative": "Los dardos te alcanzan. Sufres 2d4 de daño. La puerta está abierta.", "entryDataOptions": [{"id": 1, "description": "Entrar", "outcome": {"type": "GoToEntry", "entry": 140}}, {"id": 2, "description": "Continuar al norte", "outcome": {"type": "GoToEntry", "entry": 125}}], "entryDataRules": []}],
    [125, {
